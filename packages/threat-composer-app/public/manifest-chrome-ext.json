{
  "name": "Threat Composer viewer",
  "description": "View a Threat Composer JSON export in Threat Composer",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "js": ["contentScript.js"],
      "matches": ["https://*/*.json*", "http://*/*.json*"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["code_catalyst_inject_script.js"],
      "matches": ["https://codecatalyst.aws/*"]
    }
  ],
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "permissions": ["storage", "tabs"],
  "action": {
    "default_title": "Threat Composer",
    "default_popup": "index.html",
    "width": 800,
    "height": 600
  }
}
